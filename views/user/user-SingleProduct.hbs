<!--================Single Product Area =================-->
<div class="product_image_area">
	<div class="container">
		<div class="row s_product_inner">
			<div class="col-lg-6">
				<div class="s_Product_carousel">
					<div class="single-prd-item">
						<img class="img-fluid" src="/admin/product-Images/{{proDetails.Image.[0]}}" alt="">
					</div>
					<div class="single-prd-item">
						<img class="img-fluid" src="/admin/product-Images/{{proDetails.Image.[1]}}" alt="">
					</div>
					<div class="single-prd-item">
						<img class="img-fluid" src="/admin/product-Images/{{proDetails.Image.[2]}}" alt="">
					</div>
				</div>
			</div>
			<div class="col-lg-5 offset-lg-1">
				<div class="s_product_text">
					<h3>{{proDetails.Pname}}</h3>
					<h2>Rs.{{proDetails.Price}}</h2>
					<ul class="list">
						<li><a class="active" href="#"><span>Category</span> {{proDetails.Category}}</a></li>
						<li><a href="#"><span>Availibility</span> : In Stock</a></li>
					</ul>
					<p>{{proDetails.Description}}</p>
					<div class="product_count">
						<label for="qty">Quantity:</label>
						<input type="text" name="qty" id="sst" maxlength="12" value="1" title="Quantity:"
							class="input-text qty">
						<button
							onclick="var result = document.getElementById('sst'); var sst = result.value; if( !isNaN( sst )) result.value++;return false;"
							class="increase items-count" type="button"><i class="lnr lnr-chevron-up"></i></button>
						<button
							onclick="var result = document.getElementById('sst'); var sst = result.value; if( !isNaN( sst ) &amp;&amp; sst > 0 ) result.value--;return false;"
							class="reduced items-count" type="button"><i class="lnr lnr-chevron-down"></i></button>
					</div>
					<div class="card_area d-flex align-items-center">
						<button class="primary-btn" onclick="addToCart('{{proDetails._id}}')" style="border:none;">Add to Cart</button>
						{{!-- <a class="icon_btn" href="#"><i class="lnr lnr lnr-diamond"></i></a> --}}
						<button class="icon_btn" onclick="addToWishList('{{proDetails._id}}')" style="border:none;"><i
								class="lnr lnr lnr-heart"></i></button>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!--================End Single Product Area =================-->

<!--================Product Description Area =================-->
<section class="product_description_area">
	<div class="container">
		<ul class="nav nav-tabs" id="myTab" role="tablist">
			<li class="nav-item">
				<a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab"
					aria-controls="profile" aria-selected="false">Specification</a>
			</li>
			<li class="nav-item">
				<a class="nav-link active" id="review-tab" data-toggle="tab" href="#review" role="tab"
					aria-controls="review" aria-selected="false">Reviews</a>
			</li>
		</ul>
		<div class="tab-content" id="myTabContent">
			<div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
				<div class="table-responsive">
					<table class="table">
						<tbody>
							<tr>
								<td>
									<h5>Product Name</h5>
								</td>
								<td>
									<h5>{{proDetails.Pname}}</h5>
								</td>
							</tr>
							<tr>
								<td>
									<h5>Brand Name</h5>
								</td>
								<td>
									<h5>{{proDetails.Bname}}</h5>
								</td>
							</tr>
							<tr>
								<td>
									<h5>Category</h5>
								</td>
								<td>
									<h5>{{proDetails.Category}}</h5>
								</td>
							</tr>
							<tr>
								<td>
									<h5>Description</h5>
								</td>
								<td>
									<h5>{{proDetails.Description}}</h5>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
			<div class="tab-pane fade show active" id="review" role="tabpanel" aria-labelledby="review-tab">
				<div class="row">
					<div class="col-lg-6">
						{{#each reviews}}
						<div class="review_list">
							<div class="review_item">
								<div class="media">
									<div class="d-flex">
										<img src="img/product/review-1.png" alt="">
									</div>
									<div class="media-body">
										<h4>{{this.Name}}</h4>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
										<i class="fa fa-star"></i>
									</div>
								</div>
								<p>{{this.Review}}</p>
							</div>

						</div>
						{{/each}}
					</div>
					<div class="col-lg-6">
						<div class="review_box">
							{{#if userDetails}}
							<h4>Add a Review</h4>
							<form class="row contact_form" action="/product-review/{{proDetails._id}}" method="post"
								id="contactForm" novalidate="novalidate">
								<div class="col-md-12">
									<div class="form-group">
										<textarea class="form-control" name="Review" id="Review" rows="1"
											placeholder="Review" onfocus="this.placeholder = ''"
											onblur="this.placeholder = 'Review'"></textarea></textarea>
									</div>
								</div>
								<div class="col-md-12 text-right">
									<button type="submit" value="submit" class="primary-btn">Submit Now</button>
								</div>
							</form>
							{{/if}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<!--================End Product Description Area =================-->

